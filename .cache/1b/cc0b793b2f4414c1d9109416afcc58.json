images/image-hero-mobile.jpg) top center no-repeat;\r\n  background-size: contain;\r\n}\r\n\r\nnav {\r\n  width: 100%;\r\n  height: 50px;\r\n  @include flex-row-space-between;\r\n  padding: 2em 1.5em;\r\n  background: transparent;\r\n}\r\n\r\n.container {\r\n  @include flex-column-center;\r\n  padding-top: 190px;\r\n  background: white;\r\n}\r\n\r\n.header-wrapper {\r\n  background-color: white;\r\n  text-align: center;\r\n  padding-top: 2rem;\r\n  img {\r\n    position: absolute;\r\n    top: 26%;\r\n    left: 45%;\r\n  }\r\n  h1 {\r\n    padding: 1em;\r\n  }\r\n  p {\r\n    padding-bottom: 2em;\r\n  }\r\n  .button-wrapper {\r\n    @include flex-row-space-between;\r\n  }\r\n  #bookmark-button {\r\n    background-image: url(../../images/icon-bookmark.svg);\r\n    background-position: center center;\r\n    background-repeat: no-repeat;\r\n    border-radius: 50%;\r\n    width: 50px;\r\n    height: 50px;\r\n  }\r\n  #main-button {\r\n    padding: 1em 3em;\r\n  }\r\n}\r\n\r\n.statistics {\r\n  list-style: none;\r\n  @include flex-column-center;\r\n  .statistics-item:nth-child(2) {\r\n    border-top: 1px solid $dark-gray;\r\n    border-bottom: 1px solid $dark-gray;\r\n    padding: 0.5em 0;\r\n  }\r\n  .statistics-item:nth-child(1) {\r\n    padding-bottom: 0.5em;\r\n  }\r\n  .statistics-item:nth-child(3) {\r\n    padding: 0.5em 0;\r\n  }\r\n  .statistics-item {\r\n    @include flex-column-center;\r\n    line-height: 1.5;\r\n  }\r\n}\r\n\r\n.progres-bar-container {\r\n    width: 100%;\r\n    height: 10px;\r\n    background: lightgray;\r\n    border-radius: $border-button;\r\n    .progres-bar {\r\n        background: $moderate-cyan;\r\n        border-radius: $border-button;\r\n        height: 10px;\r\n        width: 80%;\r\n        opacity: 1;\r\n    }\r\n }\r\n\r\n.section-paragraph:first-of-type {\r\n    padding: 1.5em 0;\r\n}\r\n\r\n.section-paragraph:last-of-type {\r\n    padding-bottom: 1.5em;\r\n}\r\n\r\n.reward-title {\r\n    @include flex-column-left;\r\n}\r\n\r\n.reward-paragraph {\r\n    padding: 1em 0;\r\n}\r\n\r\n.reward-items-left {\r\n    display: flex;\r\n    align-items: center;\r\n    padding-bottom: .5em;\r\n    span {\r\n        padding-left: .5em;\r\n    }\r\n}\r\n\r\n#stock-wrapper{\r\n    opacity: .3;\r\n}\r\n\r\n.attribution {\r\n  font-size: 11px;\r\n  text-align: center;\r\n}\r\n\r\n.attribution a {\r\n  color: hsl(228, 45%, 44%);\r\n}\r\n","scss\\typography.scss":"@use \"utilities\" as *;\r\n\r\nbody {\r\n    font-family: $font-family;\r\n}\r\n\r\n.button {\r\n    color: white;\r\n    font-size: 12px;\r\n}\r\n\r\n#main-button {\r\n    font-size: 16px;\r\n    font-weight: $font-bold;\r\n}\r\n\r\nh1 {\r\n    font-size: $font-main-heading;\r\n    font-weight: $font-bold;\r\n}\r\n\r\np {\r\n    font-size: $font-para-mobile-size;\r\n    color: $dark-gray;\r\n    font-weight: $font-regular;\r\n}\r\n\r\n.statistics-item {\r\n    font-size: $font-statistics-item;\r\n    font-weight: $font-bold;\r\n    span {\r\n        font-size: $font-para-mobile-size;\r\n        font-weight: $font-regular;\r\n        color: $dark-gray;\r\n    }\r\n}\r\n\r\n.section-title {\r\n    font-size: $font-section-title-size;\r\n}\r\n\r\n.reward-title {\r\n    font-size: $font-reward-title-size;\r\n    line-height: 1.5;\r\n    span {\r\n        color: $moderate-cyan;\r\n        font-size: $font-para-mobile-size;\r\n    }\r\n}\r\n\r\n.reward-items-left {\r\n    font-size: $font-statistics-item;\r\n    span {\r\n        font-size: $font-para-mobile-size;\r\n        color: $dark-gray;\r\n    }\r\n}","scss\\components.scss":"@use \"utilities\" as *;\r\n\r\n.wrapper,\r\n.reward-wrapper {\r\n    width: 90%;\r\n    padding: 2em 1em;\r\n    border: 1px solid transparent;\r\n    box-shadow: 0px 1px 15px lightgray;\r\n    border-radius: $border-section;\r\n    margin-bottom: 1.5em;\r\n}\r\n.reward-wrapper {\r\n    margin: 0 auto;\r\n    width: 100%;\r\n    padding: 1.5em;\r\n    margin-bottom: 1.5em;\r\n}\r\n\r\n.button {\r\n    background: $moderate-cyan;\r\n    color: white;\r\n    padding: 1em 2em;\r\n    border: none;\r\n    border-radius: $border-button;\r\n}\r\n\r\n.stock-button {\r\n    background: $dark-gray;\r\n}"},"lineCount":null}},"error":null,"hash":"d579f19b6706bbcb6952f9ffce2c1b12","cacheData":{"env":{}}}